citHeader("To cite 'anominate' in publications use:")

citEntry(entry = "Manual",
  title        = "Alpha-NOMINATE: Ideal Point Estimator",
  author       = personList(as.person("Royce Carroll"),
                   as.person("Christopher Hare"),
                   as.person("Jeffrey B. Lewis"),
                   as.person("James Lo"),
                   as.person("Keith T. Poole"),
		   as.person("Howard Rosenthal")),
  year         = "2017",
  note	       = "R package version 0.6",

  textVersion  =
  paste("Royce Carroll, Christopher Hare, Jeffrey B. Lewis, James Lo, Keith T. Poole and Howard Rosenthal (2017).",
        "Alpha-NOMINATE: Ideal Point Estimator.",
        "R package version 0.6. URL http://k7moa.com/alphanominate.htm")
)

